{
  "name": "webhook-microservice",
  "version": "0.1.0",
  "description": "webhook-microservice",
  "author": {
    "email": "aox.mariano@gmail.com",
    "name": "Aronis Mariano"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aaronorosen/webhook-micoservice.git"
  },
  "scripts": {
    "start": "node --max-old-space-size=460 src/index.js",
    "lint": "eslint ./src --ext .js",
    "lint-fix": "eslint ./src --ext .js --fix",
    "mocha-test": "cross-env-shell MOCHA_REPORTER?=spec mocha --timeout 30000 --exit --reporter ${MOCHA_REPORTER}",
    "test-component": "npm run mocha-test --file 'test/component/setup.spec.js' -- \"test/component/*.spec.js\"",
    "test-integration": "npm run mocha-test --file 'test/unit/setup.spec.js' -- \"test/integration/*.spec.js\"",
    "test-all": "npm run test-component",
    "pretest-all": "npm run lint",
    "pretest": "shx rm -rf coverage",
    "test": "nyc --check-coverage npm run test-all"
  },
  "dependencies": {
    "async": "^3.1.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.12",
    "cheerio": "^1.0.0-rc.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "lodash": "^4.17.15",
    "lodb": "^0.5.2",
    "read-pkg-up": "^6.0.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env-default": "^5.1.3-1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-security": "^1.4.0",
    "mocha": "^7.1.2",
    "nyc": "^14.1.1",
    "shx": "^0.3.2",
    "spec": "^1.0.1",
    "supertest": "^3.4.2"
  }
}
